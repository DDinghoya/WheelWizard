                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red600}">
                                <TextBlock Text="600" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red700}">
                                <TextBlock Text="700" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red800}">
                                <TextBlock Text="800" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red900}">
                                <TextBlock Text="900" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red950}">
                                <TextBlock Text="950" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                        </StackPanel>

                        <!-- Yellow Scale -->
                        <StackPanel Spacing="2">
                            <Border Width="60" Height="30" Background="{DynamicResource Yellow50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Yellow100}">
                                <TextBlock Text="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Yellow200}">
                                <TextBlock Text="200" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Yellow300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Yellow400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Yellow500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center"/>
