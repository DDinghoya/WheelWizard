<ResourceDictionary  xmlns="https://github.com/avaloniaui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.PreviewWith>
        <Border Padding="20">
            <StackPanel Spacing="20">
                <!-- Gray Scale -->
                <StackPanel>
                    <TextBlock Text="Gray Scale" FontWeight="Bold" Margin="0,0,0,5"/>
                    <StackPanel Orientation="Horizontal" Spacing="5">
                        <StackPanel Spacing="2">
                            <Border Width="60" Height="30" Background="{DynamicResource Gray50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray100}">
                                <TextBlock Text="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray200}">
                                <TextBlock Text="200" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray600}">
                                <TextBlock Text="600" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray700}">
                                <TextBlock Text="700" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray800}">
                                <TextBlock Text="800" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray900}">
                                <TextBlock Text="900" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Gray950}">
                                <TextBlock Text="950" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                        </StackPanel>

                        <!-- Red Scale -->
                        <StackPanel Spacing="2">
                            <Border Width="60" Height="30" Background="{DynamicResource Red50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red100}">
                                <TextBlock Text="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Red200}">
