                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger600}">
                                <TextBlock Text="600" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger700}">
                                <TextBlock Text="700" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger800}">
                                <TextBlock Text="800" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger900}">
                                <TextBlock Text="900" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger950}">
                                <TextBlock Text="950" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                        </StackPanel>

                        <!-- Warning Scale -->
                        <StackPanel Spacing="2">
                            <TextBlock HorizontalAlignment="Center">Warning</TextBlock>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning100}">
                                <TextBlock Text="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning200}">
                                <TextBlock Text="200" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning600}">
                                <TextBlock Text="600" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning700}">
                                <TextBlock Text="700" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning800}">
                                <TextBlock Text="800" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning900}">
                                <TextBlock Text="900" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Warning950}">
                                <TextBlock Text="950" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                        </StackPanel>

                        <!-- Primary Scale -->
                        <StackPanel Spacing="2">
                            <TextBlock HorizontalAlignment="Center">Primary</TextBlock>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary100}">
                                <TextBlock Text="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary200}">
                                <TextBlock Text="200" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary600}">
