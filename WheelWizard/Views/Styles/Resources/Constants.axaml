                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Primary950}">
                                <TextBlock Text="950" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                        </StackPanel>
                    </StackPanel>

                    <Border Width="255" Height="30" Background="{DynamicResource Black}" Margin="0,2,0,0">
                        <TextBlock Text="Black" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                    </Border>   
                </StackPanel>
            </StackPanel>
        </Border>
    </Design.PreviewWith>

    <!-- Color Resources -->
    
        <!-- Neutral Scale -->
        <Color x:Key="Neutral50">#F6F7F9</Color>
        <Color x:Key="Neutral100">#EDEDF1</Color>
        <Color x:Key="Neutral200">#D7D9E0</Color>
        <Color x:Key="Neutral300">#B4B8C5</Color>
        <Color x:Key="Neutral400">#8B91A5</Color>
        <Color x:Key="Neutral500">#6C7389</Color>
        <Color x:Key="Neutral600">#575D72</Color>
        <Color x:Key="Neutral700">#474B5D</Color>
        <Color x:Key="Neutral800">#3E424E</Color>
        <Color x:Key="Neutral900">#363944</Color>
        <Color x:Key="Neutral950">#24252D</Color>
        <Color x:Key="Black">#090A0C</Color>

        <!-- Red Scale -->
        <Color x:Key="Danger50">#FFF1F4</Color>
        <Color x:Key="Danger100">#FFE0E6</Color>
        <Color x:Key="Danger200">#FFC5CF</Color>
        <Color x:Key="Danger300">#FF9DAF</Color>
        <Color x:Key="Danger400">#FF6581</Color>
        <Color x:Key="Danger500">#FE2048</Color>
        <Color x:Key="Danger600">#EC163D</Color>
        <Color x:Key="Danger700">#C70E2F</Color>
        <Color x:Key="Danger800">#A4102B</Color>
        <Color x:Key="Danger900">#881429</Color>
        <Color x:Key="Danger950">#4A0511</Color>

        <!-- Warning Scale -->
        <Color x:Key="Warning50">#FFFEE7</Color>
        <Color x:Key="Warning100">#FFFDC1</Color>
        <Color x:Key="Warning200">#FFF686</Color>
        <Color x:Key="Warning300">#FFE941</Color>
        <Color x:Key="Warning400">#FFD80D</Color>
        <Color x:Key="Warning500">#FFC800</Color>
        <Color x:Key="Warning600">#D19200</Color>
        <Color x:Key="Warning700">#A66802</Color>
        <Color x:Key="Warning800">#89510A</Color>
        <Color x:Key="Warning900">#74420F</Color>
        <Color x:Key="Warning950">#442204</Color>

        <!-- Primary Scale -->
        <Color x:Key="Primary50">#EEFFFA</Color>
        <Color x:Key="Primary100">#C7FFF0</Color>
        <Color x:Key="Primary200">#8EFFE2</Color>
        <Color x:Key="Primary300">#34EAC5</Color>
        <Color x:Key="Primary400">#01CBA5</Color>
        <Color x:Key="Primary500">#00A488</Color>
        <Color x:Key="Primary600">#00A488</Color>
        <Color x:Key="Primary700">#03826E</Color>
        <Color x:Key="Primary800">#08675A</Color>
        <Color x:Key="Primary900">#0C554A</Color>
        <Color x:Key="Primary950">#00342F</Color>
    
        <Color x:Key="BlackIsh">#090A0C</Color>
    

        <!-- IMPORTANT -->
        <!-- Any color that is going to be re-used in different components, and should ALWAYS stay the same. they can be defined here -->
        <!-- This is to ensure that if we ever want to change a color, we only have to change it in one place -->
        <SolidColorBrush x:Key="BackgroundColor" Color="{StaticResource Neutral950}" />
        <SolidColorBrush x:Key="FrameColor" Color="{StaticResource Neutral900}" />
        <SolidColorBrush x:Key="BackgroundLineColor" Color="{StaticResource Neutral700}" />
    
        <!-- Text Brushes -->
        <SolidColorBrush x:Key="BodyTextColor" Color="{StaticResource Neutral400}" />
        <SolidColorBrush x:Key="TitleTextColor" Color="{StaticResource Neutral400}" />
        <SolidColorBrush x:Key="TitleIconColor" Color="{StaticResource Neutral500}" />
        <SolidColorBrush x:Key="TitleTextHoverColor" Color="{StaticResource Neutral300}" />
        <SolidColorBrush x:Key="FormFieldLabelColor" Color="{StaticResource Neutral300}" />
        <SolidColorBrush x:Key="IntenseTitleColor" Color="{StaticResource Neutral50}" />
    
        <!-- Layout Constants -->
        <CornerRadius x:Key="GlobalCornerRadius">6</CornerRadius>
        <CornerRadius x:Key="WindowCornerRadius">0</CornerRadius>
        <CornerRadius x:Key="WindowCornerRadiusLeft">0,0,0,0</CornerRadius>
        <CornerRadius x:Key="WindowCornerRadiusBottomLeft">0,0,0,0</CornerRadius>
        <CornerRadius x:Key="WindowCornerRadiusLeftRightTwix">6,0,0,0</CornerRadius>

        <!-- Margins and Padding -->
        <Thickness x:Key="EdgeGap">10</Thickness>
        <Thickness x:Key="NegativeEdgeGap">-10</Thickness>
        <Thickness x:Key="LeftEdgeGap">10,0,0,0</Thickness>
        <Thickness x:Key="RightEdgeGap">0,0,10,0</Thickness>
        <Thickness x:Key="LeftTopEdgeGap">10,10,0,0</Thickness>
        <Thickness x:Key="SidebarSectionGap">10,20,10,0</Thickness>

        <!-- Grid Constants -->
        <GridLength x:Key="TopRowSize">95</GridLength>
    
        <!-- Button Resources -->
        <CornerRadius x:Key="ButtonCornerRadius">4</CornerRadius>
    
        <SolidColorBrush x:Key="ButtonBackground" Color="{StaticResource Neutral700}" />
        <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonForeground" Color="{StaticResource Neutral300}" />
    
        <SolidColorBrush x:Key="ButtonPointerOverBackground" Color="{StaticResource Neutral600}" />
        <SolidColorBrush x:Key="ButtonPointerOverBorderBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonPointerOverForeground" Color="{StaticResource Neutral300}" />
    
        <SolidColorBrush x:Key="ButtonPressedBackground" Color="{StaticResource Neutral500}" />
        <SolidColorBrush x:Key="ButtonPressedBorderBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonPressedForeground" Color="{StaticResource Neutral300}" />
    
        <SolidColorBrush x:Key="ButtonDisabledBackground" Color="{StaticResource Neutral800}" />
        <SolidColorBrush x:Key="ButtonDisabledBorderBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonDisabledForeground" Color="{StaticResource Neutral950}" />
</ResourceDictionary>