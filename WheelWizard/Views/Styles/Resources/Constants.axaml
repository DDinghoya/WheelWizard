<ResourceDictionary  xmlns="https://github.com/avaloniaui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.PreviewWith>
        <Border Padding="20">
            <StackPanel Spacing="20">
                <!-- Neutral Scale -->
                <StackPanel>
                    <TextBlock Text="Color Scale" FontWeight="Bold" Margin="0,0,0,5"/>
                    <StackPanel Orientation="Horizontal" Spacing="5">
                        <StackPanel Spacing="2">
                            <TextBlock HorizontalAlignment="Center">Neutral</TextBlock>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral100}">
                                <TextBlock Text="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral200}">
                                <TextBlock Text="200" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral300}">
                                <TextBlock Text="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral400}">
                                <TextBlock Text="400" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral500}">
                                <TextBlock Text="500" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral600}">
                                <TextBlock Text="600" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral700}">
                                <TextBlock Text="700" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral800}">
                                <TextBlock Text="800" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral900}">
                                <TextBlock Text="900" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Neutral950}">
                                <TextBlock Text="950" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                            </Border>
                        </StackPanel>

                        <!-- Red Scale -->
                        <StackPanel Spacing="2">
                            <TextBlock HorizontalAlignment="Center">Danger</TextBlock>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger50}">
                                <TextBlock Text="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Width="60" Height="30" Background="{DynamicResource Danger100}">
